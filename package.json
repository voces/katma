{
	"name": "katma",
	"version": "1.2.1",
	"description": "Minimum viable product of Bulldog client",
	"scripts": {
		"build": "node scripts/build.js",
		"dev": "node scripts/build.js --watch",
		"jest": "jest --silent",
		"lint": "eslint src --ext .js,.ts,.jsx,.tsx --quiet --fix",
		"prepublishOnly": "npm run build & npm run typecheck",
		"start": "node --experimental-modules --no-warnings server/src/index.js",
		"test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
		"test-lint": "eslint src --ext .js,.ts,.jsx,.tsx --max-warnings 0",
		"test": "jest",
		"typecheck": "tsc --noEmit"
	},
	"license": "ISC",
	"author": "verit",
	"main": "public/server.cjs",
	"files": [
		"public/*"
	],
	"devDependencies": {
		"@babel/preset-env": "^7.15.8",
		"@babel/preset-typescript": "^7.15.0",
		"@types/enzyme": "^3.10.10",
		"@types/jest": "^27.0.2",
		"@types/jsdom": "^16.2.13",
		"@types/lodash-es": "^4.17.5",
		"@types/marked": "^3.0.2",
		"@types/node": "^16.11.4",
		"@types/three": "^0.133.1",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-preact-pure": "^3.1.0",
		"esbuild": "^0.13.9",
		"eslint": "^7.32.0",
		"eslint-config-verit": "^4.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"jest": "^27.3.1",
		"jest-ts-webcompat-resolver": "^1.0.0",
		"jsdom": "^18.0.0",
		"prettier": "^2.4.1",
		"three": "^0.133.1",
		"ts-jest": "^27.0.7",
		"typescript": "^4.4.4",
		"webcraft": "^5.0.0"
	},
	"type": "module"
}
